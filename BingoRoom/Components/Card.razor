<table class="table table-info">
    @for(int i = 0; i < Squares.Count / 5; i++)
    {
    <tr>
        <td>@Squares[(i * 5) + 0]</td>
        <td>@Squares[(i * 5) + 1]</td>
        @if ((i * 5) + 2 == 12)
        {
            <td>FREE</td>
        }
        else
        {
            <td>@Squares[(i * 5) + 2]</td>
        }
        <td>@Squares[(i * 5) + 3]</td>
        <td>@Squares[(i * 5) + 4]</td>
    </tr>
    }
</table>

@code {
    [Parameter] public IGame.ICard MyCard { get; set; }
    private Dictionary<int, bool> _checked;
    private List<int> Squares;

    protected override void OnInitialized()
    {
        Squares = MyCard.Squares.ToList<int>();
        Squares.Insert(12, -1);
    }
}
